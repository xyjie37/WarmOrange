  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#9e9e9e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comment - Python Docstring",
      "scope": [
        "string.quoted.triple.python",
        "string.quoted.docstring.multi.python",
        "string.quoted.docstring.multi.python punctuation.definition.string.begin",
        "string.quoted.docstring.multi.python punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#9e9e9e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string.quoted", "string.unquoted"],
      "settings": {
        "foreground": "#5c7a45"
      }
    },
    {
      "name": "String - f-string",
      "scope": ["string.quoted.f"],
      "settings": {
        "foreground": "#4a6540"
      }
    },
    {
      "name": "String - r-string",
      "scope": ["string.quoted.r"],
      "settings": {
        "foreground": "#5c7a45"
      }
    },
    {
      "name": "String - b-string",
      "scope": ["string.quoted.b"],
      "settings": {
        "foreground": "#4a6540"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#c2410c"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#dc2626"
      }
    },
    {
      "name": "None",
      "scope": ["constant.language.none"],
      "settings": {
        "foreground": "#9ca3af"
      }
    },
    {
      "name": "Keyword - Control Flow",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#F31518",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword - Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#c2410c"
      }
    },
    {
      "name": "Keyword - Declaration",
      "scope": ["keyword.declaration", "storage.type"],
      "settings": {
        "foreground": "#15803d"
      }
    },
    {
      "name": "Keyword - Import",
      "scope": ["keyword.import"],
      "settings": {
        "foreground": "#15803d"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function"],
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Function - Built-in",
      "scope": ["support.function.builtin"],
      "settings": {
        "foreground": "#dc2626"
      }
    },
    {
      "name": "Function - Method",
      "scope": ["meta.method"],
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class", 
        "storage.class",
        "entity.name.type.class",
        "support.class",
        "entity.name.class.python"
      ],
      "settings": {
        "foreground": "#d69e2e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class - Inherited",
